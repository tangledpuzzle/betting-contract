  "scripts": {
    "solhint": "solhint 'contracts/*.sol'",
    "solhint:fix": "solhint 'contracts/*.sol' --fix",
    "local:node": "LOCAL_DEV=true npx hardhat node --tags fare,spin-testnet --hostname 0.0.0.0",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier:sol": "npx prettier --write 'contracts/**/*.sol'",
    "prettier:ts": "npx prettier --write 'scripts/**/*.ts'",
    "prettier": "npm run prettier:sol && npm run prettier:ts",
    "deploy": "npx hardhat deploy",
    "deploy:localchain": "LOCAL_DEV=true npx hardhat --network localchain deploy",
    "deploy:subnet": "LOCAL_DEV=true npx hardhat --network testsubnet deploy",
    "deploy:testchain": "LOCAL_DEV=true npx hardhat --network testchain deploy",
    "coverage": "RUNNING_TESTS=true npx hardhat coverage --solcoverjs ./.solcover.js --network hardhat",
    "size:contracts": "npx hardhat size-contracts",
    "export:abis": "npx hardhat export-abi",
    "generate:types": "sh bin/gen-server-types.sh",
    "gen:privkey": "openssl rand -hex 32",
    "docker:build": "docker build --no-cache -t fp-smart-contracts .",
    "reset:localchain": "rm -rf deployments/localchain",
    "reset:testchain": "rm -rf deployments/testchain",
    "test": "RUNNING_TESTS=true npx hardhat test --network hardhat",
    "test:spin": "RUNNING_TESTS=true npx hardhat test test/fare-spin.test.ts --network hardhat",
    "test:token": "RUNNING_TESTS=true npx hardhat test test/fare-token.test.ts --network hardhat",
    "simulate": "npx hardhat node > /dev/null 2>&1 & npx hardhat run scripts/simulations/simulate.ts --network localhost && kill -9 $(lsof -t -i:8545)"
  },
